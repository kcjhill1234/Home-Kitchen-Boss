<body>
  <h1>Home Kitchen Boss</h1>
  <div class="row header ">
    <div class="col s4">
      <img id="htmlImage" src="/images/boss.png" class="icon">
    </div>
    <form class="col s8">
      <div class="row">
        <div class="input-field col s12">
          <label>Diet Choice</label>
          <select id="dietChoices">
            <option value="">Not Specified</option>
          </select>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <label>Cuisine Choice</label>
          <select id="cuisineChoices">
            <option value="">Not Specified</option>
          </select>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <label>Food Allergies</label>
          <select multiple id="foodAllergies">
            <option value="">None Specific</option>
          </select>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="searchTxt" type="text" placeholder="Search.." name="search">
          <button id="searchBtn" class="waves-effect waves-light btn" type="button"><i class="material-icons">search</i> Search</button>
        </div>
      </div>
    </form>
  </div>

  <div class="row">
    <div class="col s12">
      <div id="results">
        
      </div>
    </div>
  </div>

  <div id="userModal" class="modal">
    <div class="model-content">
      <h5>Please enter your Name</h5>
      <input size= "5" id="username" type="text">
    </div>
    <div class="model-footer">
      <button class="modal-close waves-effect btn">Okay</button>
    </div>
  </div>


</body>